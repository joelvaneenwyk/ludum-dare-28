  		  //   interleaved shadow map rendering
  		  return float4(vTex2D(ShadowTex, ShadowTex_Sampler, ScreenTex).yyy, 1.0);
  		#else
  		  return float4(vTex2D(ShadowTex, ShadowTex_Sampler, ScreenTex).xyz, 1.0);
  		#endif		
  	}
	
