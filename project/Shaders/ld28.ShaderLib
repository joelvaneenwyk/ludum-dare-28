{                                              
  VS_OUT Out = (VS_OUT)0; 

  float3 objectSpaceNormal = In.Normal;
	  
  #ifndef USE_SKINNING
	  #if defined(PROJECTOR_UV) && defined(PERMUTATION_Terrain)
	    float4 ObjPos = float4(In.ObjPos.x,In.ObjPos.y,In.ObjPos.z,1.0);
