    Out.UV0AND1.x = In.UV0.x;
    Out.UV0AND1.y = In.UV0.y;
  #endif
  
  #ifdef USE_COLOR
    Out.Color = In.Color;
  #endif
  
  #ifdef USE_UV1  
    Out.UV0AND1.zw = In.UV1.xy * LightmapScaleOfs.xy+LightmapScaleOfs.zw;
  #endif

  #ifdef USE_SKINNING
    TRANSFORM_OBJECT_NORMAL(objectSpaceNormal, In.Normal, In.BoneWeight);
  #endif

